{
  "name": "angular-seed",
  "private": true,
  "version": "0.0.0",
  "description": "A starter project for AngularJS",
  "repository": "https://github.com/angular/angular-seed",
  "main": "src/main.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm --prefix ./devtools/mock install ./devtools/mock",
    "prebuild:dev": "node build.js",
    "build:dev": "webpack --config devtools/webpack/webpack.config",
    "postbuild:dev": "concurrently \"webpack-dev-server --config devtools/webpack/webpack.config\" \"nodemon devtools/mock/app/app.js --config devtools/mock/app/nodemon.json \"",
    "prestart": "npm install",
    "prebuild:code": "node build.js",
    "build:code": "webpack --config devtools/webpack/webpack.config",
    "build:component": "webpack --config devtools/webpack/webpack.config --env.environment=component",
    "postbuild:component": "concurrently \"webpack-dev-server --config devtools/webpack/webpack.config --env.environment=component\" \"node devtools/mock/app/app.js\"",
    "prebuild:prod": "node build.js",
    "build:prod": "webpack --config devtools/webpack/webpack.config --env.environment=prod",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "dependencies": {
    "angular": "1.6.9",
    "angular-chart.js": "^1.1.1",
    "angular-cookies": "1.6.9",
    "angular-route": "1.6.9",
    "angular-animate": "1.6.9",
    "angular-translate": "2.17.0",
    "angular-ui-bootstrap": "^2.5.6",
    "c3": "^0.6.6",
    "chart.js": "^2.7.3",
    "chartjs-plugin-deferred": "^1.0.1",
    "chartjs-plugin-zoom": "^0.6.6",
    "crypto-js": "^3.1.9-1",
    "jsoneditor": "^5.24.7",
    "lodash": "",
    "material-components-web": "^0.36.0",
    "xbem": "0.0.5",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "bower": "^1.7.7",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.3.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "merge-jsons-webpack-plugin": "^1.0.15",
    "mini-css-extract-plugin": "^0.4.0",
    "mkdirp": "^0.5.1",
    "ng-annotate-webpack-plugin": "^0.3.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.9.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4"
  }
}
