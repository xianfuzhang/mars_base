{
  "name": "angular-seed",
  "private": true,
  "version": "0.0.0",
  "description": "A starter project for AngularJS",
  "repository": "https://github.com/angular/angular-seed",
  "license": "MIT",
  "scripts": {
    "prebuild:dev": "webpack --config devtools/webpack/webpack.config",
    "build:dev": "webpack-dev-server --config devtools/webpack/webpack.config",
    "prestart": "npm install",
    "build:component": "webpack --config devtools/webpack/webpack.config --env.environment=component",
    "postbuild:component": "webpack-dev-server --config devtools/webpack/webpack.config --env.environment=component",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "dependencies": {
    "angular": "1.6.9",
    "angular-cookies": "1.6.9",
    "angular-route": "1.6.9",
    "angular-translate": "2.17.0",
    "angular-ui-bootstrap": "^2.5.6",
    "lodash": "",
    "material-components-web": "^0.36.0",
    "xbem": "0.0.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "bower": "^1.7.7",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "rimraf": "^2.6.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "merge-jsons-webpack-plugin": "^1.0.15",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
